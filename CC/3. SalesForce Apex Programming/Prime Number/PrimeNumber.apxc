public class PrimeNumber {
    public Integer num { get; set; }
    public Boolean isPrime { get; set; }
    
    public void checkPrime() {
        isPrime = true;
        if (num <= 1) {
            isPrime = false;
        } else {
            for (Integer i = 2; i <= Math.sqrt(num); i++) {
                if (Math.mod(num, i) == 0) {
                    isPrime = false;
                    break;
                }
            }
        }
    }
}
